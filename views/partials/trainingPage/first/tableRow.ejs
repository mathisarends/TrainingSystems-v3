<tr class="table-row mainExercise">
    <td>
      <% const exerciseDataItem= exerciseData[exerciseIndex]; %>
      
        <select name="week1_day<%= dayNumber %>_exercise<%= exerciseNumber %>_category" class="exercise-category-selector">
          <% for (const category of exerciseCategories) { %>
            <option value="<%= category %>" <%= (exerciseData[exerciseIndex]?.category ?? '') === category ? 'selected' : '' %>>
              <%= category %>
            </option>
          <% } %>
        </select>
      </td>

      <td class="exercise-name-selector-td">
        
        <select
          name="week1_day<%= dayNumber %>_exercise<%= exerciseNumber %>_exercise_name"
