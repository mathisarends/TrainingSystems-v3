          style="<%= exerciseDataItem?.category === '- Bitte Auswählen -' ? '' : 'display: none;' %>">
          <% for (const exercise of categorizedExercises["- Bitte Auswählen -"]) { %>
            <option value="<%= exercise %>" <%=exerciseDataItem?.exercise === exercise ? 'selected' : '' %>>
              <%= exercise %>
            </option>
          <% } %>
        </select>
    
        <select
          name="week1_day<%= dayNumber %>_exercise<%= exerciseNumber %>_exercise_name"
