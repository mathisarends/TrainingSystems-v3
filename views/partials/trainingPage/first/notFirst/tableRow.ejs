<tr class="table-row mainExercise">
  <td>
      <% const exerciseDataItem=exerciseDataNew[exerciseIndex] || exerciseData[exerciseIndex]; %>
      <% if (exerciseDataItem?.category && exerciseDataItem?.category !=="- Bitte Ausw채hlen -" ) { %>
        <select name="week<%= weekNumber %>_day<%= dayNumber %>_exercise<%= exerciseNumber %>_category"
          class="exercise-category-selector">
          <% for (const category of exerciseCategories) { %>
            <option value="<%= category %>" <%=exerciseDataItem?.category===category ? 'selected' : '' %>>
              <%= category %>
            </option>
            <% } %>
        </select>
        <% } else { %>
          <select name="week<%= weekNumber %>_day<%= dayNumber %>_exercise<%= exerciseNumber %>_category"
            class="exercise-category-selector">
            <% for (const category of exerciseCategories) { %>
              <option value="<%= category %>">
                <%= category %>
              </option>
              <% } %>
          </select>
          <% } %>
  </td>

