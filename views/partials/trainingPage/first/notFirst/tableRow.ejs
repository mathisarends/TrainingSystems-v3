      <% for (const exercise of categorizedExercises["- Bitte Ausw채hlen -"]) { %>
        <option value="<%= exercise %>" <%=exerciseDataItem?.exercise === exercise ? 'selected' : '' %>>
          <%= exercise %>
        </option>
      <% } %>
    </select>

    <select
