  <td><input type="number" class="sets" name="week<%= weekNumber %>_day<%= dayNumber %>_exercise<%= exerciseNumber %>_sets"
      value="<%= exerciseDataNew[exerciseIndex]?.sets || exerciseData[exerciseIndex]?.sets %>">
