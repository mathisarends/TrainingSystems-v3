<div class="volumePhases">
  <table class="calculate-table">
    <thead>
      <tr class="table-header-row">
        <th>Übung</th>
        <th>Minimale Satzanzahl/Woche</th>
        <th>Maximalle Satzanzahl / Woche</th>
        <th>Median</th>
        <th>Absolvierte Sätze</th>
        <th>Tonnage</th>
      </tr>
    </thead>
    <tbody>
      <!--nur für Abstand kriege das anders nicht hin-->
      <tr class="table-row volume-row">
        <td>a</td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <tr class="table-row volume-row">
        <td class="category-name">SQUAT</td>
        <td>
          <input type="number" class="squatmev" value="<%=squatmev%>" readonly required />
        </td>
        <td>
          <input type="number" class="squatmrv" value="<%=squatmrv%>" readonly required />
        </td>
        <td>
          <input type="number" readonly name="squat-median-volume" class="squat-median-volume-result">
        </td>
        <td>
          <input type="number" readonly class="squat-sets-done">
        </td>
        <td>
          <input type="number" readonly class="squat-tonnage">
        </td>
      </tr>
      <tr class="table-row volume-row">
        <td class="category-name">BENCH</td>
        <td>
          <input type="number" class="benchmev" value="<%=benchmev%>" readonly required />
        </td>
        <td>
          <input type="number" class="benchmrv" value="<%=benchmrv%>" readonly required />
        </td>
        <td>
          <input type="number" readonly name="bench-median-volume" class="bench-median-volume-result">
        </td>
        <td>
          <input type="number" readonly class="bench-sets-done">
        </td>
        <td>
          <input type="number" readonly class="bench-tonnage">
        </td>
      </tr>
      <tr class="table-row volume-row">
        <td class="category-name">DEADLIFT</td>
        <td>
          <input type="number" class="deadliftmev" value="<%=deadliftmev%>" readonly required />
